import java.util.Arrays;

public class Main {
    public boolean isSubSetSum(int[] arr, int n, int target) {
        if (target == 0) {
            return true;
        }
        if (n == 0) {
            return false;
        }
        if (arr[n - 1] > target) {
            return isSubSetSum(arr, n - 1, target);
        }
        return isSubSetSum(arr, n - 1, target) || isSubSetSum(arr, n - 1, target - arr[n - 1]);
    }

    public static void main(String[] args) {
        int[] arr = {3, 34, 4, 12, 5, 2};
        int target = 9;
        int n = arr.length;

        Main obj = new Main();
        boolean result = obj.isSubSetSum(arr, n, target);
        System.out.println("Subset with given sum exists: " + result);
    }
}
